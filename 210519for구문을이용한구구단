public class GuGuDanTest {
	public static void main(String[] args) {
		System.out.println("1. for 구문을 활용한 구구단");
			int i;
			for (i =2; i <= 9; i++) {
				System.out.print(i + "단\t");
			}
			System.out.println();	
				int j;
				int k;
				for (k = 1; k <= 9; k++) {
					for (j = 2; j <= 9; j++) {	
						System.out.print(j + "*" + k + "=" + j*k + "\t");
					}
					System.out.println();	
				}
	}
}
