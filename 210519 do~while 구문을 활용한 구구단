public class GuGuDanTest {
	public static void main(String[] args) {
		System.out.println("3. do~while 구문을 활용한 구구단");
			int r = 2;
			do {
				System.out.print(r + "단\t");
				r++;
			} while(r <= 9);
			System.out.println();
			int s = 2;
			int t = 1;
				do	{
					do {
					System.out.print(s + "*" + t + "=" + s*t + "\t");
					s++;
					} while(s <=9);
					System.out.println();
					s = 2;
					t++;
				} while (t <= 9);
	}
}			
